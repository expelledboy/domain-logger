{
  "name": "@expelledboy/domain-logger",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "format": "prettier --write ./src"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "prettier": "3.3.3"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 75
      }
    }
  }
}